size(600,400);

// 2 points
strokeWeight(3);
point(50,50);
strokeWeight(5);
point(100,80);
// line
strokeWeight(2);
line(150, 50, 250, 150);

// rectangle
noFill();
rect(300, 50, 120, 80);

// circle
fill(200,0,0);
ellipse(100,250,60,60);

// ellipse
fill(0, 0, 200);
ellipse(200,250,100,60);

// triangle
fill(0,200,0);
triangle(350,200,300,300,400,300);

// quadrilateral
fill(255, 255, 255);
quad(450, 200, 520, 180, 540, 300, 470, 320);
